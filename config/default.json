{
  "device": {
    "use_cuda": true,
    "cuda_version": "12.9",
    "gpu_model": "NVIDIA GeForce GTX 1650",
    "vram_mb": 4096,
    "compute_capability": "7.5",
    "recommended_resolution": "1080p",
    "max_batch_size": 1,
    "fallback_to_cpu": true,
    "cpu_threads": 8
  },
  "rife": {
    "model": "rife-v4.6-lite",
    "interpolation_mode": "fast",
    "inference_args": {
      "UHD": false,
      "scale": 1.0,
      "fp16": false,
      "tta": false,
      "time_step": 0.5
    }
  },
  "3d_rendering": {
    "fov_limit": 90,
    "rotation_limit_x": 45,
    "rotation_limit_y": 45,
    "rotation_limit_z": 15,
    "warp_method": "perspective"
  },
  "performance": {
    "max_concurrent_tasks": 2,
    "memory_optimized": true,
    "cache_size_mb": 1024,
    "preload_clips": true
  },
  "logging": {
    "log_level": "info",
    "log_file": "fpv_plugin.log",
    "verbose": false
  }
}